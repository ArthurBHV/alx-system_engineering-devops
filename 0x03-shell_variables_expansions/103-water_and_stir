#!/bin/bash

# Define the strings
string1=""
string2="b"

# Get the lengths of the strings using expr
length1=$(expr length "$string1")
length2=$(expr length "$string2")

# Compare the lengths and print the result
if [ "$length1" -eq "$length2" ]; then
  echo "Lengths match: $length1 characters"
else
  echo "Lengths do not match"
fi

